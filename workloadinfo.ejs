<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
        "http://www.w3.org/TR/html4/loose.dtd">
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=0">
    <title>工作量查询</title>
    <link rel="stylesheet" href="./weui/dist/style/weui.css"/>
    <!--<link rel="stylesheet" href="./weui/dist/example/example.css">-->
</head>


<body ontouchstart>
<div class="page">
    <div class="page__hd">
        <h1 class="page__title">本学年工作量</h1>
    </div>
    <div class="page__bd">
        <div class="weui-cells">
            <div class="weui-cell">
                <div class="weui-cell__bd">
                    <p>工作量比例</p>
                </div>
                <div class="weui-cell__ft">100%</div>
            </div>
            <div class="weui-cell">
                <div class="weui-cell__bd">
                    <p>协助导师姓名</p>
                </div>
                <div class="weui-cell__ft">无</div>
            </div>
            <div class="weui-cell">
                <div class="weui-cell__bd">
                    <p>协助导师工作量比例</p>
                </div>
                <div class="weui-cell__ft">0</div>
            </div>
            <div class="weui-cell">
                <div class="weui-cell__bd">
                    <p>人数</p>
                </div>
                <div class="weui-cell__ft">18</div>
            </div>
            <div class="weui-cell">
                <div class="weui-cell__bd">
                    <p>班号</p>
                </div>
                <div class="weui-cell__ft">*****</div>
            </div>
            <div class="weui-cell">
                <div class="weui-cell__bd">
                    <p>备注</p>
                </div>
                <div class="weui-cell__ft">。。。。。。</div>
            </div>

        </div>

    </div>


</div>

</div>
</body>
</html>