                                    <h4 class="weui-media-box__title"><%= students2014[i].name%></h4>
                                    <p class="weui-media-box__desc"><%= students2014[i].type_name%>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= students2014[i].degree_type_name%>
                                        &nbsp;&nbsp;&nbsp; <% if (students2014[i].ktconfirm == 1) {%>
                                        已开题
                                        <%} else {%>
                                        未开题
                                        <%}%>
                                        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= students2016[i].telephone%>
                                    </p>
                                </div>
                            </div>
                        </div>
                        <%}%>
                    </div>


                </div>
            </div>





        </div>
        <div id="tab2" class="weui-tab__bd-item">
            <header class='demos-header'>
                <h1 class="demos-title">非应届学生</h1>
            </header>
            <div class="page__bd">
            <div class="weui-cells__title">2013级</div>
            <div class="weui-panel__bd">
                <% for(var i = 0;i<students2013.length;i++) {%>
                <div class="weui-panel weui-panel_access">
                <div class="weui-media-box weui-media-box_appmsg">
                    <div class="weui-media-box__bd">
