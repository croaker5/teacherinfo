<body ontouchstart>


<div class="weui-tab">
    <div class="weui-tab__bd">
        <div id="tab1" class="weui-tab__bd-item weui-tab__bd-item--active">
            <header class='demos-header'>
                <h1 class="demos-title">应届学生</h1>
            </header>
            <div class="page">
                <div class="page__bd">
                    <div class="weui-cells__title">2016级</div>
                    <div class="weui-panel__bd">
                        <% for(var i = 0;i<students2016.length;i++) {%>
                        <div class="weui-panel weui-panel_access">
                            <div class="weui-media-box weui-media-box_appmsg">
                                <div class="weui-media-box__bd">
                                    <h4 class="weui-media-box__title"><%= students2016[i].name%></h4>
                                    <p class="weui-media-box__desc"><%= students2016[i].type_name%>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= students2016[i].degree_type_name%>
                                        &nbsp;&nbsp;&nbsp; <% if (students2016[i].ktconfirm == 1) {%>
                                        已开题
                                        <%} else {%>
                                        未开题
                                        <%}%>
                                        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= students2016[i].telephone%>
                                    </p>
                                </div>
                            </div>
                        </div>
                        <%}%>
                    </div>
                    <div class="weui-cells__title">2015级</div>
                    <div class="weui-panel__bd">
                        <% for(var i = 0;i<students2015.length;i++) {%>
