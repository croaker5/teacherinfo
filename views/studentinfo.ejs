                        <p class="weui-media-box__desc"><%= students2008[i].type_name%>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= students2008[i].degree_type_name%>
                            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= students2008[i].telephone%>
                        </p>
                    </div>
                </div>
                </div>
                <%}%>
            </div>
            <div class="weui-cells__title">2007级</div>
            <div class="weui-panel__bd">
                <% for(var i = 0;i<students2007.length;i++) {%>
                <div class="weui-panel weui-panel_access">
                <div class="weui-media-box weui-media-box_appmsg">
                    <div class="weui-media-box__bd">
                        <h4 class="weui-media-box__title"><%= students2007[i].name%></h4>
                        <p class="weui-media-box__desc"><%= students2007[i].type_name%>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= students2007[i].degree_type_name%>
                            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= students2007[i].telephone%>
                        </p>
                    </div>
                </div>
                </div>
                <%}%>
            </div>
            </div>

        </div>
        <div id="tab3" class="weui-tab__bd-item">
            <header class='demos-header'>
                <h1 class="demos-title">答辩状态</h1>
            </header>
            <div class="page">
                <div class="page__bd">
                    <div class="weui-cells__title">2014级</div>
                    <div class="weui-cells">
                        <% for(var i=0;i<students2014.length;i++) {%>
                        <div class="weui-cell">
                            <div class="weui-cell__bd">
                                <p><%= students2014[i].name%></p>
                            </div>
                            <div class="weui-cell__ft">
                                <% if (students2014[i].ktconfirm == 1) {%>
                                已开题
                                <%} else {%>
                                未开题
                                <%}%>
                            </div>
                        </div>
                        <% }%>
                    </div>

                </div>
                <div class="page__bd">
                    <div class="weui-cells__title">2013级</div>
                    <div class="weui-cells">
                        <% for(var i=0;i<students2013.length;i++) {%>
                        <div class="weui-cell">
                            <div class="weui-cell__bd">
                                <p><%= students2013[i].name%></p>
                            </div>
                            <div class="weui-cell__ft">
                                <% if (students2013[i].ktconfirm == 1) {%>
